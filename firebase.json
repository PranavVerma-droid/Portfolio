{
  "hosting": {
    "site": "portfolio-pranavvv",
    "public": "main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules.json",
    "indexes": "firestore.indexes.json"
